#!/usr/bin/env groovy

pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                echo 'Built'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
                echo 'Tested'
            }
        }
        stage('Deploy'){
            steps {
                echo 'Deploying...'
                echo 'Deployed'
            }
        }
    }
}
